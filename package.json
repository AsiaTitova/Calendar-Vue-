{
  "name": "vuex-ts-calendar-project",
  "version": "0.1.0",
  "author": "Titova Anastasia",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "fake-json": "json-server --watch ./src/api/Calendar.json"
  },
  "dependencies": {
    "@braintree/sanitize-url": "5.0.2",
    "@vue/component-compiler-utils": "1.3.1",
    "@vue/eslint-config-prettier": "6.0.0",
    "axios": "0.21.4",
    "babel-plugin-transform-decorators": "6.24.1",
    "chromedriver": "85.0.1",
    "commitizen": "4.2.4",
    "core-js": "3.19.0",
    "date-fns": "2.25.0",
    "draggable": "4.2.0",
    "eslint-plugin-prettier": "3.4.1",
    "moment": "2.29.1",
    "node-sass": "4.14.1",
    "resize-observer-polyfill": "1.5.1",
    "v-calendar": "2.3.4",
    "vue": "2.6.14",
    "vue-click-outside": "1.1.0",
    "vue-property-decorator": "9.1.2",
    "vue-class-component": "^7.2.6",
    "vue-router": "3.1.6",
    "vuedraggable": "2.24.3",
    "vuex": "3.6.2",
    "vuex-module-decorators": "1.2.0",
    "json-server": "0.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/cli-plugin-babel": "4.5.17",
    "@vue/cli-plugin-eslint": "4.5.17",
    "@vue/cli-plugin-router": "4.5.14",
    "@vue/cli-plugin-typescript": "4.5.15",
    "@vue/cli-service": "4.5.14",
    "@vue/compiler-sfc": "3.2.20",
    "@vue/eslint-config-standard": "4.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "babel-eslint": "10.1.0",
    "core-js": "3.19.0",
    "cz-customizable": "6.3.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "husky": "7.0.4",
    "prettier": "2.4.1",
    "prettier-eslint": "11.0.0",
    "sass-loader": "8.0.2",
    "selenium-server": "3.141.59",
    "typescript": "4.1.6",
    "vue-template-compiler": "2.6.14",
    "webpack": "4.46.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      },
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "vue/v-bind-style": "error",
      "vue/v-on-style": "error",
      "no-unused-expressions": "error",
      "no-console": "warn",
      "no-debugger": "warn",
      "no-empty": "error",
      "vue/html-closing-bracket-spacing": "error",
      "vue/html-end-tags": "error",
      "vue/html-self-closing": "off",
      "vue/name-property-casing": "error",
      "vue/no-multi-spaces": "error",
      "vue/prop-name-casing": "error",
      "vue/this-in-template": "error",
      "vue/camelcase": "off",
      "vue/eqeqeq": "error",
      "space-before-function-paren": "off",
      "standard/computed-property-even-spacing": "off",
      "no-unused-vars": "off",
      "no-prototype-builtins": "warn"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "eslintIgnore": [
    "/dist/",
    "/tests/unit/coverage/",
    "src/vendor/libs/**/*.js",
    "src/layout",
    "src/components/Widgets/Widget.vue",
    "src/components/",
    "src/vendor",
    "src/components/Core/KernelProject/FQL"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
    }
  }
}
